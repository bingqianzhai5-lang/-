{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react-jsx",
    "noEmit": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}